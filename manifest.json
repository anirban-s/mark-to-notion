{
  "manifest_version": 3,
  "name": "Mark To Notion",
  "version": "1.0",
  "description": "This extension save to Notion!",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": ["notifications"],
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://api.notion.com/*"]
}
